{
	"name": "Meet Statistics",
	"description": "Track user engagement in Google Meet by monitoring attendance, text messages, reactions, hands up and camera/voice activity",
	"version": "1.0.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup/index.html",
		"default_icon": {
			"16": "img/16.png",
			"24": "img/24.png",
			"32": "img/32.png",
			"48": "img/48.png",
			"64": "img/64.png",
			"128": "img/128.png"
		}
	},
	"options_page": "main/index.html",
	"permissions": ["storage", "unlimitedStorage", "activeTab"],
	"content_scripts": [
		{
			"js": [
				"utils/Store.js",
				"script/Participant.js",
				"script/Meeting.js",
				"script/index.js"
			],
			"matches": [
				"*://meet.google.com/*-*-*"
			]
		}
	],
	"icons": {
		"16": "img/16.png",
		"24": "img/24.png",
		"32": "img/32.png",
		"48": "img/48.png",
		"64": "img/64.png",
		"128": "img/128.png"
	},
	"author": {
		"email": "timotejroiko@yahoo.com.br"
	},
	"homepage_url": "https://github.com/timotejroiko/meet-statistics",
	"minimum_chrome_version": "85"
}
